    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "env":{
                "PYTHONPATH": "F:/CodeProjects/2024/starfab/frameworks/scdatatools;${workspaceFolder}"
            },
            "cwd": "${workspaceFolder}"

        },
        {
            "name": "Launch UI",
            "type": "debugpy",
            "request": "launch",
            "program": "app/ui.py",
            "console": "integratedTerminal",
            "env":{
                "PYTHONPATH": "F:/CodeProjects/2024/starfab/frameworks/scdatatools;${workspaceFolder}"
            },
            "cwd": "${workspaceFolder}"

        }
    ]
}